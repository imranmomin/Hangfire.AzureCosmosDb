{
  "compilerOptions": {
    "experimentalDecorators": false,
    "lib": [ "es6", "dom" ],
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noStrictGenericChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "es2015"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "storedprocedure/*.ts"
  ]
}
